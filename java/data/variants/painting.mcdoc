use ::java::server::util::text::Text

#[since="1.21"]
dispatch minecraft:resource[painting_variant] to struct Painting {
	asset_id: #[id(registry="texture", path="painting")] string,
	/// Dimension in blocks.
	width: int @ 1..16,
	/// Dimension in blocks.
	height: int @ 1..16,
	#[since="1.21.2"] title: Text,
	#[since="1.21.2"] author: Text,
}
