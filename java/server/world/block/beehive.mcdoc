use ::java::server::util::effect::EffectId
use super::super::entity::AnyEntity

dispatch minecraft:block[beehive,bee_nest] to struct Beehive {
	...super::BlockEntity,
	#[until="1.20.5"]
	FlowerPos: FlowerPos,
	#[since="1.20.5"]
	flower_pos: int[] @ 3,
	#[until="1.20.5"]
	Bees: [LegacyBee],
	#[since="1.20.5"]
	bees: [Bee],
}

struct FlowerPos {
	X: int,
	Y: int,
	Z: int,
}

struct LegacyBee {
	MinOccupationTicks: int,
	TicksInHive: int,
	EntityData: AnyEntity,
}
struct Bee {
	min_ticks_in_hive: int,
	ticks_in_hive: int,
	entity_data: AnyEntity,
}