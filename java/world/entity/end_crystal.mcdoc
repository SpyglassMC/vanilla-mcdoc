dispatch minecraft:entity[end_crystal] to struct EndCrystal {
	...super::EntityBase,
	/// Whether to show the base of the end crystal.
	ShowBottom?: boolean,
	/// Coordinates that the beam is pointing to
	#[until="1.20.5"]
	BeamTarget?: BeamTarget,
	/// Coordinates that the beam is pointing to
	#[since="1.20.5"]
	beam_target?: int[] @ 3,
}

struct BeamTarget {
	X?: int,
	Y?: int,
	Z?: int,
}
